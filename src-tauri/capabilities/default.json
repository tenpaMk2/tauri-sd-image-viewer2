{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"dialog:default",
		{
			"identifier": "fs:read-all",
			"allow": ["/**"]
		},
		{
			"identifier": "fs:write-all",
			"allow": ["/**"]
		},
		{
			"identifier": "fs:scope",
			"allow": ["/**"]
		}
	]
}
